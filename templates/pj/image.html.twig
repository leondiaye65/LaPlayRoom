{# {{ dump(field.value) }} #}
{% for image in field.value %}
    <a href="#" class="ea-lightbox-thumbnail" data-featherlight="{{ image.id }}"
       data-featherlight-close-on-click="anywhere">
        <img src="{{ vich_uploader_asset(image, 'imageFile') }}" class="img-fluid"
             alt="{{ image.name }}" width="150">
    </a>

    <div id=" {{ image.id }}" class="ea-lightbox">
        <img src="{{ vich_uploader_asset(image, 'imageFile') }}" alt="{{ image.name }}" class="img-fluid">

    </div>
{% endfor %}